<Layout  title="Kerala Startup Mission">
	
  <main>
    <!-- page-banner-area-start -->
    <div class="page-banner-area page-banner-height" data-background="/assets/im/jpeg/inn.jpg">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="page-banner-content text-center">
                        <h3>Join IEDC</h3>
                        <p>Be part of the largest network of aspiring entrepreneurs...<br>Be a change maker...</p>
                        <div class="page-bottom-btn mt-55">
                            <a href="https://forms.gle/BsoT8LMnsGTpdQQn7" class="st-btn-4" target="_blank">Students</a>
                        
                            <a href="https://forms.gle/7M9oMkuqS3KsdNwP8" class="st-btn-4" target="_blank">Researchers</a>
                           
                            <a href="https://forms.gle/m5T3unxc3wEXivKV6" class="st-btn-4" target="_blank">Faculties</a>
                                                
                            <a href="https://forms.gle/gDGd198XccSKUXq46" class="st-btn-4" target="_blank">Interested Individuals</a>
                        </div>                            
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- page-banner-area-end -->
    <!--Counter-->
               

    <!-- Join Us -->
    <div class="services-area pt-70 light-bg-s pb-50">
        <div class="container">
            
