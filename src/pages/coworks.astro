---
import Layout from '../layouts/Layout.astro';
import Cta from '../components/Cta.astro';
---

<Layout  title="Kerala Startup Mission">
	
  <main>
    <!-- page-banner-area-start -->
    <div class="page-banner-area page-banner-height" data-background="/assets/im/jpeg/coworks.jpg">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="page-banner-content text-center">
                        <h3>LEAP Coworks in IEDCs</h3>
                        <p><b>Launch Empower Accelerate Prosper</b></p>
                        <p>The LEAP coworking space aims to create an environment that nurtures innovation, 
                            encourages networking, and facilitates the success of startups and entrepreneurs.</p>
                        <div class="page-bottom-btn mt-55">
                            <a href="https://docs.google.com/document/d/1sBczBj2lcRXF_X7KRIQwnJzaJPFyNVygLV9bB_EIWkc/edit?usp=sharing" 
                            class="st-btn-4" target="_blank">EoI Document</a>
                        
                            <a href="https://docs.google.com/document/d/1rjXpxoWXU6oWuWZ5ibX_VdfMol9XSqPJ/edit?usp=sharing&ouid=111361519005395749414&rtpof=true&sd=true" 
                            class="st-btn-4" target="_blank">Application Form Template</a>
                           
                            <a href="https://forms.gle/VCEG66iGMn8oMMrh7" class="st-btn-4" target="_blank">Apply Now</a>
                            <a href="https://leap.startupmission.in/" class="st-btn-4" target="_blank">Know more</a>                                              
                            
                        </div>                            
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- page-banner-area-end -->
    <!--Counter-->
               

    <!-- Join Us -->
    <div class="services-area pt-70 light-bg-s pb-50">
        <div class="container">
            
            <div class="row">
                <div class="col-xl-12">
                    
                </div>
            </div>
            <div class="row mt-40">
                <div class="row align-items-center">
                    
                    <div class="col-xl-6 col-lg-6">
                        <div class="abs-section-title text-left">
                            <span>Join the network of coworking spaces pan Kerala</span>
                            <h4>Join LEAP Coworks</h4>
                            <p>Last Date to submit EOI: 10 September 2023</p>
                        </div>
                            <div class="about-content">
                                    <p>
                                        Kerala Startup Mission (KSUM) has been at the forefront of supporting startups and entrepreneurs 
                                        in Kerala through its incubation centres. Now, we are going to transform our incubation centres, 
                                        partner incubation centres, and IEDC into a vibrant coworking space called LEAP Coworks. LEAP, 
                                        which stands for Launch Empower Accelerate Prosper, embodies our commitment to fostering 
                                        innovation, collaboration, and success in the startup ecosystem.
                                    </p>
                                    <p>
                                        Kerala Startup Mission is widening its opportunities to IEDCs to be part of the LEAP initiative. 
                                        IEDCs meeting the minimum requirements or willing to provide the requirements shall be on boarded 
                                        as LEAP centres
                                    </p>
                                    
                            </div>
                    </div>
                    <div class="col-xl-6 col-lg-6">
                               <div class="about-image w-img">
                                       <img src="/assets/im/jpeg/leap3.jpg" alt=""><br>
                                       <img src="/assets/im/jpeg/leap1.png" alt="">
                               </div>
                       </div>
               </div>
                
        </div>
    </div>
    <!-- sJoin us-end -->

    <!-- technolgy-index-start -->
    <div class="technolgy-index mt-70 mb-10">
        <div class="container">
        <div class="row">
            <div class="services-area pt-70 light-bg-s pb-50">
				<div class="container">						
						
						<div class="row mt-40">
								<div class="col-xl-3 col-lg-3 col-md-6">
										<div class="services-item mb-30">
										<button type="button" class="wc-checkout d-flex align-items-center justify-content-center w-100">
										<a href="https://docs.google.com/document/d/1sBczBj2lcRXF_X7KRIQwnJzaJPFyNVygLV9bB_EIWkc/edit?usp=sharing">Expression of Interest Document</a>
												</button>
										</div>
								</div>
								<div class="col-xl-3 col-lg-3 col-md-6">
										<div class="services-item mb-30">
										<button type="button" class="wc-checkout d-flex align-items-center justify-content-center w-100" >
										<a href="https://docs.google.com/document/d/1rjXpxoWXU6oWuWZ5ibX_VdfMol9XSqPJ/edit?usp=sharing&ouid=111361519005395749414&rtpof=true&sd=true">Application form template</a>
												</button>
										</div>
								</div>
								<div class="col-xl-3 col-lg-3 col-md-6">
										<div class="services-item mb-30">
										<button type="button" class="wc-checkout d-flex align-items-center justify-content-center w-100">
										<a href="https://forms.gle/VCEG66iGMn8oMMrh7">Submit Expression of Interest</a>
												</button>
										</div>
								</div>
															

						</div>
				</div>
		</div>
		<!-- features__area-end -->
                                
        </div>
    </div>
    
    
    <!-- List of Students -->
    <div class="modal fade" id="productModalId" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered product__modal" role="document">
            <div class="modal-content">
                <div class="product__modal-wrapper p-relative">
                    <div class="product__modal-close p-absolute">
                        <button data-bs-dismiss="modal"><i class="fal fa-times"></i></button>
                    </div>
                    <div class="product__modal-inner">
                        <div class="row">
                        <iframe src="https://docs.google.com/spreadsheets/d/e/2PACX-1vS1cPL2TADdXA1Z-Gdt04_mTFOAVi42z899Xl97sZpeKtkQ9zDaK-0lfc400mhWbRfn_rGFT9_yWqwU/pubhtml?gid=1845074038&amp;single=true&amp;widget=true&amp;headers=false"></iframe>
                        </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- List of IEDC modal end -->
    <!-- List of Researchers modal start -->
    <div class="modal fade" id="productModalId1" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered product__modal" role="document">
            <div class="modal-content">
                <div class="product__modal-wrapper p-relative">
                    <div class="product__modal-close p-absolute">
                        <button data-bs-dismiss="modal"><i class="fal fa-times"></i></button>
